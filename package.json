{
  "name": "@actionishope/shelley",
  "version": "1.0.0-alpha.2",
  "license": "ISC",
  "description": "React UI library, another one. This one uses 'stylable' from Wix for full 'themeability'.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/action-is-hope/shelley.git"
  },
  "keywords": [
    "a11y",
    "stylable",
    "react",
    "typescript"
  ],
  "author": "action-is-hope<info@action-is-hope.earth>",
  "bugs": {
    "url": "https://github.com/action-is-hope/shelley/issues"
  },
  "homepage": "https://github.com/action-is-hope/shelley#readme",
  "main": "indexLib.js",
  "types": "indexLib.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "**/*.ts",
    "components/**/*.js",
    "indexLib.js",
    "**/*.map",
    "**/*.st.css"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "copy": "copyfiles package.json \"./package.json\" \"./README.md\" dist",
    "prebuild": "npm run clean",
    "build": "webpack --mode production --no-devtool",
    "build-dist": "npm run clean && npm run copy && tsc --project tsconfig-npm.json && npm run build-components-stcss && npm run build-components-styles-stcss && npm run build-components-site-stcss",
    "build-components-stcss": "stc --srcDir=\"./src/components\" --outDir=\"./dist/components\" --stcss",
    "build-components-styles-stcss": "stc --srcDir=\"./src/styles\" --outDir=\"./dist/styles\" --stcss",
    "build-components-site-stcss": "stc --srcDir=\"./src/components-site\" --outDir=\"./dist/components-site\" --stcss",
    "lint": "eslint . -f codeframe",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint && jest && npm run cypress",
    "unit-test": "jest",
    "cypress": "cypress run --component",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@react-aria/utils": "^3.14.2",
    "@react-stately/tree": "^3.4.1",
    "@react-types/menu": "^3.7.3",
    "@react-types/overlays": "^3.6.5",
    "react-aria": "^3.22.0",
    "react-stately": "^3.20.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@stylable/cli": "^5.7.1",
    "@stylable/core": "^5.7.1",
    "@stylable/jest": "^5.7.1",
    "@stylable/runtime": "^5.7.1",
    "@stylable/webpack-plugin": "^5.7.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.5",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/react-test-renderer": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "copyfiles": "^2.4.1",
    "cypress": "^10.8.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-stylable": "^4.13.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "match-sorter": "^6.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "storybook-addon-rtl": "^0.4.4",
    "ts-dedent": "^2.2.0",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3"
  }
}
