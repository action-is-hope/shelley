{
  "name": "@actionishope/shelley",
  "version": "0.1.1-alpha.13",
  "license": "ISC",
  "description": "Gatsby, stylable boilerplate.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/action-is-hope/shelley.git"
  },
  "keywords": [
    "a11y",
    "stylable",
    "react",
    "typescript"
  ],
  "author": "action-is-hope<info@action-is-hope.earth>",
  "bugs": {
    "url": "https://github.com/action-is-hope/shelley/issues"
  },
  "homepage": "https://github.com/action-is-hope/shelley#readme",
  "dependencies": {
    "@stylable/core": "1.0.2",
    "classnames": "^2.2.6",
    "create-index": "^2.5.0",
    "react-expanding-textarea": "^2.0.4"
  },
  "main": "index.js",
  "scripts": {
    "type-check": "tsc",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 1 \"./src/**/*.st.css\" \"./src/**/*.js\" dist && copyfiles package.json \"./package.json\" dist && copyfiles README.md \"./README.md\" dist",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "build-dist": "npm run clean && npm run copy && tsc --build tsconfig-npm.json && rimraf dist/layouts && rimraf dist/pages",
    "format": "prettier --parser typescript --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.ts*\"",
    "test": "jest"
  },
  "np": {
    "yarn": false,
    "anyBranch": true,
    "contents": "dist"
  },
  "devDependencies": {
    "@stylable/jest": "1.0.2",
    "@stylable/node": "1.0.2",
    "@stylable/runtime": "1.0.2",
    "@stylable/webpack-plugin": "1.0.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/react-test-renderer": "^16.9.1",
    "@types/testing-library__jest-dom": "git://github.com/vagonpidarasov/jest-dom.git",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "@vaadin/vaadin-icons": "^4.3.1",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.23",
    "copyfiles": "^2.1.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "gatsby": "^2.19.45",
    "gatsby-link": "^2.2.31",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-typescript": "^2.1.20",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-gatsby": "^1.0.19",
    "np": "^5.2.1",
    "prettier": "^1.19.1",
    "react-dom": "^16.10.1",
    "react-helmet": "^5.2.0",
    "react-syntax-highlighter": "^12.2.1",
    "react-test-renderer": "^16.12.0",
    "rimraf": "3.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  }
}
