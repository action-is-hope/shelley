{
  "name": "@actionishope/shelley",
  "version": "0.1.1-alpha.25",
  "license": "ISC",
  "description": "React UI library, another one. This one uses 'stylable' from Wix for full 'themeability'.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/action-is-hope/shelley.git"
  },
  "keywords": [
    "a11y",
    "stylable",
    "react",
    "typescript"
  ],
  "author": "action-is-hope<info@action-is-hope.earth>",
  "bugs": {
    "url": "https://github.com/action-is-hope/shelley/issues"
  },
  "homepage": "https://github.com/action-is-hope/shelley#readme",
  "dependencies": {
    "@stylable/dom-test-kit": "^3.11.9",
    "classnames": "^2.2.6",
    "react-expanding-textarea": "^2.2.3"
  },
  "main": "index.js",
  "scripts": {
    "type-check": "tsc",
    "clean": "rimraf dist",
    "copy": "copyfiles -u 1 \"./src/**/*.st.css\" \"./src/**/*.css\" \"./src/**/*.js\" dist && copyfiles package.json \"./package.json\" dist && copyfiles README.md \"./README.md\" dist",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "build-dist": "npm run clean && npm run copy && tsc --build tsconfig-npm.json && rimraf dist/layouts && rimraf dist/pages",
    "format": "prettier --parser typescript --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.ts*\"",
    "test": "jest --watch",
    "release": "np"
  },
  "np": {
    "yarn": false,
    "anyBranch": true,
    "contents": "dist"
  },
  "devDependencies": {
    "@stylable/core": "^3.9.1",
    "@stylable/jest": "^3.9.1",
    "@stylable/node": "^3.9.1",
    "@stylable/webpack-plugin": "^3.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.4.9",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.6.1",
    "@types/react": "^16.9.48",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-syntax-highlighter": "^11.0.5",
    "@types/react-test-renderer": "^16.9.3",
    "@types/testing-library__jest-dom": "git://github.com/vagonpidarasov/jest-dom.git",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@vaadin/vaadin-icons": "^4.3.1",
    "babel-jest": "^26.3.0",
    "babel-preset-gatsby": "^0.5.8",
    "copyfiles": "^2.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby": "^2.24.52",
    "gatsby-link": "^2.4.13",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-typescript": "^2.4.18",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-preset-gatsby": "^1.0.68",
    "np": "^6.4.0",
    "prettier": "^2.0.5",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-syntax-highlighter": "^12.2.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "3.0.2",
    "ts-jest": "^26.3.0",
    "typescript": "^3.9.6"
  }
}
